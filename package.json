{
  "private": true,
  "name": "unotifier-desktop",
  "version": "0.1.1",
  "repository": "git@github.com:UniversalNotification/unotifier-desktop.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "description": "",
  "main": "lib/main/index.js",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --runInBand --config jest.config.js",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "dev": "run-p dev:main dev:renderer",
    "dev:main": "tsc --project tsconfig.build.json --watch",
    "dev:renderer": "webpack serve --config webpack.dev.js",
    "start": "electron .",
    "clean": "run-p clean:*",
    "clean:main": "rimraf lib",
    "clean:renderer": "rimraf dist",
    "clean:package": "rimraf build",
    "build": "run-p build:*",
    "build:main": "tsc --project tsconfig.build.json",
    "build:renderer": "webpack --config webpack.prod.js",
    "release": "standard-version",
    "package": "electron-packager . --icon=public/icon.png --out=build --asar",
    "package:dev": "electron-packager . --icon=public/icon.png --out=build",
    "package:all": "electron-packager . --icon=public/icon.png --out=build --asar --all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint test build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/electron-packager": "^1.0.1",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/jest": "^27.4.0",
    "@types/level": "^6.0.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "copy-webpack-plugin": "^10.2.1",
    "css-loader": "^6.5.1",
    "electron": "^16.0.7",
    "eslint": "8.7.0",
    "extra-fetch": "^1.8.5",
    "extra-request": "^4.0.1",
    "husky": "^4.3.8",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.16",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-patch": "^2.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@blackglory/errors": "^2.0.0",
    "@blackglory/go": "^0.1.1",
    "@blackglory/types": "^0.6.5",
    "@delight-rpc/electron": "^2.2.0",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "classnames": "^2.3.1",
    "delight-rpc": "^1.3.0",
    "electron-is-dev": "^2.0.0",
    "electron-reloader": "^1.2.1",
    "electron-store": "^8.0.1",
    "extra-filesystem": "^0.4.1",
    "extra-generator": "^0.2.15",
    "extra-promise": "^0.21.2",
    "extra-react-hooks": "^0.1.10",
    "immer": "^9.0.12",
    "iterable-operator": "^0.14.5",
    "level": "^7.0.1",
    "micro": "^9.3.5-canary.3",
    "observable-hooks": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "return-style": "^0.12.6",
    "rxjs": "^7.5.2",
    "simplebar-react": "^2.3.6",
    "universal-notification": "^0.1.10",
    "use-immer": "^0.6.0"
  }
}
