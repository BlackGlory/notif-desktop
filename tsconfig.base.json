{
  "compilerOptions": {
    "target": "ES2019"
  , "module": "CommonJS"
  , "strict": true
  , "jsx": "react-jsx"
  , "esModuleInterop": true
  , "skipLibCheck": true
  , "baseUrl": "."
  , "paths": {
      "@src/*": ["src/*"]
    , "@test/*": ["__tests__/*"]
    , "@main/*": ["src/main/*"]
    , "@renderer/*": ["src/renderer/*"]
    , "@windows/*": ["src/main/windows/*"]
    , "@components/*": ["src/renderer/components/*"]
    }
  }
}
